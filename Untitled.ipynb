    "import numpy as np \n",
    "import matplotlib.pyplot as plt \n",
    "def y1(x):\n",
    "    return np.log(1+x)/x\n",
    "x=np.linspace(-1E,-15,1E,-15,100)\n",
    "y=y1(x)\n",
    "plt.plot(x,y,'-')\n",
    "plt.show()\n"
